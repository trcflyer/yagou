@import '../../common/common.wxss';
.container {
    background-color: #F2f2f2;
}

.newest-box .newest-list{
   /* display: inline-block;
    margin:0 5rpx 5rpx 0;
    border-radius: 10px;*/
    text-align: center;
  padding-top: 10px;
}

.newest-box .newest-list .limage{
    height: 320rpx;
    width: 100%;
    background: #fff;
}
.newest-box .newest-list .limage image{
    height: 100%;
    width: 100%;
}
.newest-box .newest-list .info{
    width: 100%;
    height: 160rpx;
    overflow: hidden;
    background: #fff;
}

.newest-box .newest-list .title{
    display: block;
    text-align: center;
    width: 100%;
    height: 33rpx;
    line-height: 33rpx;
    color: #333;
    overflow: hidden;
    font-size: 35rpx;
    margin-top: 20rpx;
}

.newest-box .newest-list .desc{
    display: block;
    text-align: center;
    position: relative;
    width: auto;
    height: 24rpx;
    line-height: 24rpx;
    overflow: hidden;
    color: #999;
    font-size: 24rpx;
    margin-top: 16rpx;
    margin-bottom: 20rpx;
}

.newest-box .newest-list .price{
    display: block;
    text-align: center;
    width: 100%;
    height: 27rpx;
    line-height: 27rpx;
    overflow: hidden;
    color: #b4282d;
    font-size: 27rpx;
}